						double ergebnis1 = 3.154 * Math.pow(10, 13) * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("Kalenderjahr") && ausgabe.equals("Millisekunde")) {
					try {
						double ergebnis1 = 3.154 * Math.pow(10, 10) * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("Kalenderjahr") && ausgabe.equals("Sekunde")) {
					try {
						double ergebnis1 = 3.154 * Math.pow(10, 7) * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("Kalenderjahr") && ausgabe.equals("Minute")) {
					try {
						double ergebnis1 = 525600 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("Kalenderjahr") && ausgabe.equals("Stunde")) {
					try {
						double ergebnis1 = 8760 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("Kalenderjahr") && ausgabe.equals("Tag")) {
					try {
						double ergebnis1 = 365 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("Kalenderjahr") && ausgabe.equals("Woche")) {
					try {
						double ergebnis1 = 52.142 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("Kalenderjahr") && ausgabe.equals("Monat")) {
					try {
						double ergebnis1 = 12 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dgft) {
						JOptionPane.showMessageDialog(null, "Sie haben keine G¸ltige Nummer eingegeben!!!");
					}
				}
			}
		});
		btnBerechnen.setBounds(11, 113, 140, 25);
		contentPane.add(btnBerechnen);
		
		JButton btnClear = new JButton("Clear");
		btnClear.setFont(new Font("Tahoma", Font.BOLD, 12));
		btnClear.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				tfEingabe.setText("");
				tfAusgabe.setText("");
			}
		});
		btnClear.setBackground(Color.ORANGE);
		btnClear.setBounds(173, 113, 100, 25);
		contentPane.add(btnClear);
	}
}
